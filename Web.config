    <rewrite>
      <rewriteMaps>
        <rewriteMap name="StaticRewrites">          
          <add key="/" value="/index.aspx" />

        </rewriteMap>
        <rewriteMap name="StaticRedirects">
          <add value="/" key="/index.aspx" />

        </rewriteMap>
      </rewriteMaps>
      <rules>
        <rule name="Rewrite Rule" stopProcessing="true">
          <match url=".*" />
          <conditions>
            <add input="{StaticRewrites:{URL}}" pattern="(.+)" />
          </conditions>
          <action type="Rewrite" url="{C:1}" appendQueryString="false" />
        </rule>
        <rule name="Redirect Rule" stopProcessing="true">
          <match url=".*" />
          <conditions>
            <add input="{StaticRedirects:{REQUEST_URI}}" pattern="(.+)" />
          </conditions>
          <action type="Redirect" url="{C:1}" appendQueryString="False" redirectType="Permanent" />
        </rule>        
        <rule name="Rewrite for faculty detail outer with param 3">
          <match url="^faculty-detail/(.*)/(.*)$" />
          <action type="Rewrite" url="facultydetail.aspx?mpgid=34&amp;pgidtrail=35&amp;ptitle={R:1}&amp;fid={R:2}" />
        </rule>
        <rule name="Rewrite for events-detail outer with param 2">
          <match url="^events-detail/(.*)/(.*)$" />
          <action type="Rewrite" url="eventsdetail.aspx?mpgid=119&amp;pgidtrail=131&amp;ptitle={R:1}&amp;eventsid={R:2}" />
        </rule>
        <rule name="Rewrite for news-detail outer with param 2">
          <match url="^news-detail/(.*)/(.*)$" />
          <action type="Rewrite" url="newsdetail.aspx?mpgid=119&amp;pgidtrail=130&amp;ptitle={R:1}&amp;eventsid={R:2}" />
        </rule>
        <rule name="Rewrite for gallery-details outer with param 2">
          <match url="^gallery-detail/(.*)/(.*)$" />
          <action type="Rewrite" url="gallerydetail.aspx?mpgid=119&amp;pgidtrail=132&amp;ptitle={R:1}&amp;gid={R:2}" />
        </rule>
      
      </rules>

    </rewrite>
